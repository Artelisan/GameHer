{% extends 'admin/admin.html.twig' %}

{% block content %}

    <div class="row">
        <div class="col col-12">
            <h1>{{ role.name }}</h1>
        </div>
        <div class="col col-9">
            <p><i class="fa fa-globe"></i> {{ role.category }}</p>
        </div>
    </div>
	{% for member in role.members %}
	<div class="row">
		<div class="col s12">
			{{ member.nickname }}
		</div>
	</div>
	{% else %}
		<div class="row">
			No members
		</div>
	{% endfor %}

{% endblock %}

